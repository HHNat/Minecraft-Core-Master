{
  "name": "minecraft-core-master",
  "version": "7.5.3",
  "description": "Minecraft-Core-Master en TypeScript: descarga, instala y lanza versiones Vanilla y modloaders (Forge, NeoForge, Fabric, Quilt, LegacyFabric). Compatible con modpacks de Modrinth (.mrpack) y con gestión completa de librerías, assets y nativos.",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mc-core": "./dist/bin/cli.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w"
  },
  "keywords": [
    "minecraft",
    "minecraft-core-master",
    "minecraft-java-core",
    "minecraft-launcher-core",
    "minecraft-node",
    "launcher",
    "mojang",
    "mojang-login",
    "microsoft",
    "microsoft-login",
    "minecraft-premium",
    "minecraft-nopremium",
    "typescript",
    "forge",
    "fabric",
    "neoforge",
    "quilt",
    "legacyfabric",
    "mods",
    "minecraft-core",
    "nova"
  ],
  "author": "NovaStepStudios <stepnicka012@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/SantiagoStepnicka2025/Minecraft-Core-Master"
  },
  "homepage": "https://gitlab.com/novastepstudios/minecraft-core-master/#readme",
  "bugs": {
    "url": "https://gitlab.com/novastepstudios/minecraft-core-master/issues"
  },
  "files": [
    "resources/**/*",
    "dist/**/*",
    "tsconfig.json",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/adm-zip": "0.5.7",
    "@types/node": "24.3.0",
    "@types/node-fetch": "^2.6.13",
    "@types/prompt": "1.1.9",
    "@types/unzipper": "0.10.11",
    "electron": "38.1.0",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "adm-zip": "0.5.16",
    "node-fetch": "2.6.13",
    "prompt": "1.3.0",
    "unzipper": "0.12.3",
    "uuid": "11.1.0"
  }
}
